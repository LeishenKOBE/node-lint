        "@types/is-ci": "^3.0.0",
        "@types/lodash": "^4.14.172",
        "@types/npm-which": "^3.0.1",
        "chalk": "^2.1.0",
        "commander": "^2.11.0",
        "debug": "^3.0.1",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.1.4",
        "execa": "^1.0.0",
        "find-cache-dir": "^3.0.0",
        "globby": "^8.0.1",
        "ignore": "^3.3.5",
        "is-ci": "^2.0.0",
