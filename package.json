{
    "name": "shi-node-lint",
    "version": "0.0.1",
    "description": "Donâ€™t think, just write!",
    "main": "lib/index.js",
    "bin": "./bin/shi-node-lint.js",
    "engines": {
        "node": ">=8.0"
    },
    "scripts": {
        "lint": "node ./bin/shi-node-lint.js",
        "lint:fix": "node ./bin/shi-node-lint.js --fix",
        "prepare": "npm run lint && npm run test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css}": [
            "npm run lint:fix",
            "git add"
        ]
    },
    "files": [
        "bin",
        "lib",
        ".eslintrc.js",
        ".prettierrc.js"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeishenKOBE/node-lint.git"
    },
    "keywords": [],
    "author": "shidongzhao",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LeishenKOBE/node-lint/issues"
    },
    "homepage": "https://github.com/LeishenKOBE/node-lint#readme",
    "dependencies": {
        "@babel/core": "^7.12.3",
        "@babel/eslint-parser": "^7.12.1",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@types/chalk": "^2.2.0",
        "@types/commander": "^2.12.2",
        "@types/debug": "^4.1.7",
        "@types/eslint": "^7.28.0",
        "@types/eslint-config-prettier": "^6.11.0",
        "@types/eslint-plugin-prettier": "^3.1.0",
